{
  "cli": {
    "version": ">= 15.0.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
       "googleServicesFile": "./google-services.json"
      }
      
    },
    "preview": {
      "distribution": "internal",

      "android": {
        "buildType": "apk",
              "googleServicesFile": "./google-services.json"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle",
        "googleServicesFile": "./google-services.json"
      }
    }
  },
  "submit": {
    "production": {}
  }
}