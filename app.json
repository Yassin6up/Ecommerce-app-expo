{
  "expo": {
    "name": "J buy",
    "slug": "j-buy",
    "version": "3.0.7",
    "orientation": "portrait",
    "icon": "./assets/icone.png",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#000000",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kreypton.jbuy",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/icone.png",
        "backgroundColor": "#000000"
      },
      "config": {
        "scrollView": {
          "keyboardDismissMode": "interactive"
        }
      }
    },
    "android": {
      "versionCode": 8,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icone.png",
        "backgroundColor": "#000000"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.knjo.buyjo",
      "intentFilters": [
        {
          "action": "VIEW",
          "category": ["DEFAULT", "BROWSABLE"],
          "data": {
            "scheme": "https",
            "host": "wa.me"
          }
        },
        {
          "action": "DIAL",
          "category": ["DEFAULT"],
          "data": {
            "scheme": "tel"
          }
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "aba96dbc-307b-43ae-80a2-c1fa8ef31cc1"
      }
    },
    "owner": "alcaranoisco"
  },
  "assetBundlePatterns": [
    "**/*"
  ]
}